{
  "name": "scroll-u",
  "version": "1.0.1",
  "description": "high-performance scrollable list component",
  "keywords": [
    "react",
    "component",
    "virtual-list",
    "infinite-scroll",
    "scroll-list"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "next dev demo",
    "prepublishOnly": "npm run build",
    "build:demo": "vite build",
    "preview": "vite preview"
  },
  "homepage": "https://github.com/akzj/scroll-u#readme",
  "bugs": {
    "url": "https://github.com/akzj/scroll-u/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akzj/scroll-u.git"
  },
  "license": "MIT",
  "author": "fu.niukey@gmail.com",
  "type": "module",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "next": "^15.3.5",
    "rollup": "^4.44.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-use-client": "^1.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0",
    "vite": "^4.5.14"
  }
}
